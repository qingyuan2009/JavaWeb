	</servlet>
	<servlet-mapping>
		<servlet-name>ServletKName</servlet-name>
		<url-pattern>/servlet_k</url-pattern>
	</servlet-mapping>
	
	<!-- Servlet 11 配置 -->
	<servlet>
		<servlet-name>ServletLName</servlet-name>
